{
  "extends": ["airbnb-base", "plugin:jest/recommended", "prettier"],
  "plugins": ["import", "jest", "prettier"],
  "rules": {
    "class-methods-use-this": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "no-await-in-loop": 0
  },
  "env": {
    "node": true,
    "jest/globals": true
  }
}
